{
"name": "Witness",
"version": "1.0",
"manifest_version": 2,
"description": "Witness This Media",
"content_scripts":[
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
],
"browser_action": {
  "default_popup": "popup.html",
  "default_title": "Witness This Media"
},
"permissions": [
  "notifications",
  "https://*/",
  "http://*/"
]
}